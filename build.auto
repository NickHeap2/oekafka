{
  "autoTargets": [
    {
      "filePattern": "build.xml",
      "runTargets": "build",
      "initialDelayMs": 1000
    },
    {
      "filePattern": "macrodefs.xml",
      "runTargets": "build",
      "initialDelayMs": 1000
    },
    {
      "filePattern": "**/test_*.{p,cls}",
      "runTargets": "test",
      "initialDelayMs": 1000
    },
    {
      "filePattern": "src/**/*.{p,w,i,cls}",
      "runTargets": "compile",
      "initialDelayMs": 1000
    }
  ]
}
